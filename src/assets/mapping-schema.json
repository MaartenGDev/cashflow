{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "Description to category mapping",
  "description": "Key-value map to link a transaction description to an category",
  "type": "object",
  "properties": {
    "mapping": {
      "patternProperties": {
        ".+": {
          "type": "string",
          "enum": [
            "Streaming",
            "Investing",
            "Food",
            "NonEssentialFood",
            "VVE",
            "Insurance",
            "Mortgage",
            "Internet",
            "Shopping",
            "Transportation",
            "Travel",
            "HouseItems",
            "Hobby",
            "Clothing",
            "Sport",
            "Tax",
            "Salary",
            "CreditCard",
            "InternalMoneyTransfer",
            "Health",
            "SocialActivities",
            "Gifts",
            "Other"
          ]
        }
      }
    }
  },
  "required": ["mapping"]
}
